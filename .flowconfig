module.name_mapper='^forms\(.*\)$' -> '<PROJECT_ROOT>/src/forms/\1'
module.name_mapper='^index\(.*\)$' -> '<PROJECT_ROOT>/src/index/\1'
module.name_mapper='^services\(.*\)$' -> '<PROJECT_ROOT>/src/services/\1'
module.name_mapper='^utils\(.*\)$' -> '<PROJECT_ROOT>/src/utils/\1'
module.name_mapper='^store\(.*\)$' -> '<PROJECT_ROOT>/src/store/\1'

[strict]
